<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:gravity="bottom"
    android:orientation="vertical"
    android:paddingStart="6dp"
    android:paddingEnd="6dp"
    tools:context="com.verifone.swordfish.manualtransaction.SupportFragments.AmountKeyboard">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:orientation="horizontal">


        <ImageButton
            android:layout_width="122dp"
            android:layout_height="90dp"
            android:background="@android:color/transparent"
            android:contentDescription="@null"
            android:scaleType="fitCenter"
            android:src="@drawable/button_3"
            android:visibility="invisible" />

        <Space
            android:layout_width="16dp"
            android:layout_height="20dp" />

        <ImageButton
            android:layout_width="122dp"
            android:layout_height="90dp"
            android:background="@android:color/transparent"
            android:contentDescription="@null"
            android:scaleType="fitCenter"
            android:src="@drawable/button_blue_122_x_89"
            android:visibility="invisible" />

    </LinearLayout>

    <Space
        android:layout_width="20dp"
        android:layout_height="15dp" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">


        <FrameLayout
            android:layout_width="122dp"
            android:layout_height="90dp">

            <ImageButton
                android:id="@+id/btnCalculatedAmount"
                android:layout_width="122dp"
                android:layout_height="90dp"
                android:background="@android:color/transparent"
                android:contentDescription="@null"
                android:scaleType="fitCenter"
                android:src="@drawable/button_blue_122_x_89"
                android:tag="-1" />

            <TextView
                android:id="@+id/textViewCalculatedAmount"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="30dp"
                android:background="@android:color/transparent"
                android:fontFamily="Roboto-Medium"
                android:text="@string/numeric_3"
                android:textAlignment="center"
                android:textColor="@color/white"
                android:textSize="24sp" />
        </FrameLayout>

        <Space
            android:layout_width="16dp"
            android:layout_height="20dp" />

        <FrameLayout
            android:layout_width="122dp"
            android:layout_height="90dp">

            <ImageButton
                android:id="@+id/buttonFive"
                android:layout_width="122dp"
                android:layout_height="90dp"
                android:background="@android:color/transparent"
                android:contentDescription="@null"
                android:scaleType="fitCenter"
                android:src="@drawable/button_blue_122_x_89"
                android:tag="5" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="30dp"
                android:background="@android:color/transparent"
                android:fontFamily="Roboto-Medium"
                android:text="@string/numeric_5"
                android:textAlignment="center"
                android:textColor="@color/white"
                android:textSize="24sp" />
        </FrameLayout>

    </LinearLayout>

    <Space
        android:layout_width="20dp"
        android:layout_height="15dp" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <FrameLayout
            android:layout_width="122dp"
            android:layout_height="90dp">

            <ImageButton
                android:id="@+id/buttonTen"
                android:layout_width="122dp"
                android:layout_height="90dp"
                android:background="@android:color/transparent"
                android:contentDescription="@null"
                android:scaleType="fitCenter"
                android:src="@drawable/button_blue_122_x_89"
                android:tag="10" />

            <TextView
                android:id="@+id/textView10"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="30sp"
                android:background="@android:color/transparent"
                android:fontFamily="Roboto-Medium"
                android:text="@string/numeric_10"
                android:textAlignment="center"
                android:textColor="@color/white"
                android:textSize="24sp" />
        </FrameLayout>

        <Space
            android:layout_width="16dp"
            android:layout_height="20dp" />

        <FrameLayout
            android:layout_width="122dp"
            android:layout_height="90dp">

            <ImageButton
                android:id="@+id/buttonTwenty"
                android:layout_width="122dp"
                android:layout_height="90dp"
                android:background="@android:color/transparent"
                android:contentDescription="@null"
                android:scaleType="fitCenter"
                android:src="@drawable/button_blue_122_x_89"
                android:tag="20" />

            <TextView
                android:id="@+id/textView20"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="30sp"
                android:background="@android:color/transparent"
                android:fontFamily="Roboto-Medium"
                android:text="@string/numeric_20"
                android:textAlignment="center"
                android:textColor="@color/white"
                android:textSize="24sp" />
        </FrameLayout>

    </LinearLayout>

    <Space
        android:layout_width="20dp"
        android:layout_height="15dp" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <FrameLayout
            android:layout_width="122dp"
            android:layout_height="90dp">

            <ImageButton
                android:id="@+id/buttonFifty"
                android:layout_width="122dp"
                android:layout_height="90dp"
                android:background="@android:color/transparent"
                android:contentDescription="@null"
                android:scaleType="fitCenter"
                android:src="@drawable/button_blue_122_x_89"
                android:tag="50"
                android:text="@string/numeric_50"
                android:textColor="@color/white" />

            <TextView
                android:id="@+id/textView50"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="30dp"
                android:background="@android:color/transparent"
                android:fontFamily="Roboto-Medium"
                android:text="@string/numeric_50"
                android:textAlignment="center"
                android:textColor="@color/white"
                android:textSize="24sp" />

        </FrameLayout>

        <Space
            android:layout_width="16dp"
            android:layout_height="20dp" />

        <FrameLayout
            android:layout_width="122dp"
            android:layout_height="90dp">

            <ImageButton
                android:id="@+id/buttonOneHundred"
                android:layout_width="122dp"
                android:layout_height="90dp"
                android:background="@android:color/transparent"
                android:contentDescription="@null"
                android:scaleType="fitCenter"
                android:src="@drawable/button_blue_122_x_89"
                android:tag="100"
                android:text="@string/numeric_100" />

            <TextView
                android:id="@+id/textView100"
                android:layout_width="match_parent"
                android:layout_height="90dp"
                android:layout_gravity="center_horizontal"
                android:layout_marginTop="30sp"
                android:background="@android:color/transparent"
                android:fontFamily="Roboto-Medium"
                android:text="@string/numeric_100"
                android:textAlignment="center"
                android:textColor="@color/white"
                android:textSize="24sp" />
        </FrameLayout>
    </LinearLayout>

    <Space
        android:layout_width="20dp"
        android:layout_height="32dp" />


</LinearLayout>
