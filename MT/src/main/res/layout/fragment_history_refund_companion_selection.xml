<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context="com.verifone.swordfish.manualtransaction.HistoryFragments.HistoryRefundCompanionSelection">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="508dp"
            android:background="@color/white"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textView29"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="203dp"
                android:text="@string/refund_history_label"
                android:textAlignment="center"
                android:textColor="@color/greyish_brown"
                android:textSize="24sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textView28"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="7dp"
                android:text="@string/this_items_will_be_refunded_textView"
                android:textAlignment="center"
                android:textColor="@color/warm_grey"
                android:textSize="20sp" />
        </LinearLayout>

        <View
            android:id="@+id/divider2"
            android:layout_width="match_parent"
            android:layout_height="2dp"
            android:background="@color/white_three" />

        <FrameLayout
            android:id="@+id/refundButtonsSelection"
            android:layout_width="match_parent"
            android:layout_height="94dp">

        </FrameLayout>
    </LinearLayout>
</FrameLayout>
