<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.verifone.swordfish.manualtransaction.HistoryFragments.HistoryRefundController">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginBottom="24sp"
        android:layout_marginTop="24sp"
        android:background="@color/white"
        android:orientation="horizontal">

        <FrameLayout
            android:id="@+id/historyRefundList"
            android:layout_width="489sp"
            android:layout_height="match_parent">

        </FrameLayout>

        <View
            android:id="@+id/divider1"
            android:layout_width="2sp"
            android:layout_height="match_parent"
            android:background="@color/white_three" />

        <LinearLayout
            android:layout_width="758sp"
            android:layout_height="match_parent"
            android:layout_marginRight="1dp"
            android:orientation="vertical">


            <FrameLayout
                android:id="@+id/refundCompanion"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

            </FrameLayout>
        </LinearLayout>
    </LinearLayout>

</FrameLayout>
